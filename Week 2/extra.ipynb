    "import csv\n",
    "\n",
    "with open('uber_eats.csv') as data_file:\n",
    "    data = csv.reader(data_file)\n",
    "\n",
    "    uber_total = 0\n",
    "    row_count = 0\n",
    "    for row in data:\n",
    "        \n",
    "        if row_count > 0:\n",
    "            uber_total += float(row[2])\n",
    "        row_count += 1\n",
    "\n",
    "    print(uber_total)\n"
   ]
  }
